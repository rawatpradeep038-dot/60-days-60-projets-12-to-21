# Server ki settings
PORT=5000
NODE_ENV=development

# Database connection (MongoDB Atlas wala string paste karo)
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/url-shortener?retryWrites=true&w=majority

# Ya local MongoDB ke liye:
# MONGODB_URI=mongodb://localhost:27017/url-shortener

# Application settings
BASE_URL=http://localhost:5000
SHORT_CODE_LENGTH=6

# Rate limiting (kitni baar API call kar sakte ho)
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100